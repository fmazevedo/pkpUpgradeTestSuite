<!DOCTYPE locales [
	<!ELEMENT locales (locale+)>
	<!ELEMENT locale EMPTY>
		<!ATTLIST locale
			key CDATA #REQUIRED
			name CDATA #REQUIRED>
]>

<locales>
	<locale key="en_US" name="English"/>
	<locale key="es_ES" name="Español (España)"/>
	<locale key="pt_BR" name="Português (Brasil)"/>
</locales>
